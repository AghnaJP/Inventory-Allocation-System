{
  "entryFile": "src/index.ts",
  "noImplicitAdditionalProperties": "throw-on-extras",
  "controllerPathGlobs": ["src/v1/**/controller/*.ts"],
  "spec": {
    "outputDirectory": "src/routes",
    "specVersion": 3,
    "specFile": "swagger.json"
  },
  "routes": {
    "routesDir": "src/routes",
    "middleware": "express"
  }
}
